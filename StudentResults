
package Sample;
import java.util.Scanner;
public class Student {
public static void main(String[] args) {
	Scanner sc= new Scanner(System.in);
	System.out.println("Please enter the marks of the Student");
	int marks=sc.nextInt();
	System.out.println("marks of the Student is  "+marks);
	if(marks>0) {
		if(marks<=35) {
			System.out.println("Student got passed with "+marks+" marks");
		}
		else
		{
			System.out.println("Student got passed with first class"+marks+" marks");
		}
	}
	else {
		System.out.println("the student got failed "+marks);
	}
	
}
}


  
